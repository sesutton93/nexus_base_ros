<?xml version="1.0" ?>

<launch>

  <!-- Bringup nexus_teleop_node -->
  <node pkg="nexus_base_ros" type="nexus_teleop_key" name="teleop_key" respawn="false" output="screen" launch-prefix="xterm -e" >
    <param name="axis_linear" value="1" type="int" />
    <param name="axis_angular" value="0" type="int" />
  </node>

  <!-- Bringup nexus_base -->
  <node pkg="rosserial_python" type="serial_node.py" name="nexus_base" respawn="false" output="screen" launch-prefix="xterm -e" >
    <param name="port" value="/dev/omni_controller" />
    <param name="baud" value="57600" />
  </node>
  
  <?ignore
  <!-- Bringup nexus_base_controller node -->
  <node pkg="nexus_base_ros" type="nexus_base_controller" name="base_controller" respawn="false" />
  ?>  

  <!-- Bringup nexus_base_controller node -->
  <node pkg="nexus_base_ros" type="nexus_base_controller" name="base_controller" respawn="false" output="screen" launch-prefix="xterm -e"/>

</launch>